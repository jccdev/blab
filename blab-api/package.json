{
  "name": "blab-api",
  "version": "1.0.0",
  "description": "Api for blab",
  "main": "dist/blab-api/src/app.js",
  "scripts": {
    "serve": "yarn run clean && yarn run build-ts && concurrently \"yarn run watch-ts\" \"yarn run watch-node\"",
    "watch-node": "nodemon --inspect ./dist/blab-api/src/app.js",
    "watch-ts": "tsc -w --sourceMap",
    "build-ts": "tsc",
    "migrate-create": "migrate create --compiler \"ts:./migrate/migrate-ts-compiler.js\" -e \".ts\" -t \"./migrate/template.ts\"",
    "migrate-up": "migrate up --compiler \"ts:./migrate/migrate-ts-compiler.js\"",
    "migrate-down": "migrate down --compiler \"ts:./migrate/migrate-ts-compiler.js\"",
    "clean": "rimraf ./dist"
  },
  "author": "jccdev",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "migrate": "1.6.2",
    "mongodb": "3.5.3",
    "ts-node": "8.6.2",
    "typescript": "3.8.2",
    "uuid": "7.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.0",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.2",
    "@types/mongodb": "3.3.16",
    "@types/node": "12.12.6",
    "@types/uuid": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "2.20.0",
    "@typescript-eslint/parser": "2.20.0",
    "concurrently": "5.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "nodemon": "2.0.2",
    "rimraf": "3.0.2"
  }
}
