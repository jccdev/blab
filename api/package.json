{
  "name": "blab-api",
  "version": "1.0.0",
  "description": "Api for blab",
  "main": "build/dist/app.js",
  "scripts": {
    "start": "concurrently \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon ./build/dist/app.js",
    "watch-ts": "tsc -w",
    "build-ts": "tsc",
    "migrate-create": "migrate create --compiler \"ts:./migrate/migrate-ts-compiler.js\" -e \".ts\" -t \"./migrate/template.ts\"",
    "migrate-up": "migrate up --compiler \"ts:./migrate/migrate-ts-compiler.js\"",
    "migrate-down": "migrate down --compiler \"ts:./migrate/migrate-ts-compiler.js\""
  },
  "author": "jccdev",
  "license": "MIT",
  "dependencies": {
    "express": "4.17.1",
    "migrate": "1.6.2",
    "mongodb": "3.5.3",
    "ts-node": "8.6.2",
    "typescript": "3.8.2"
  },
  "devDependencies": {
    "@types/express": "4.17.2",
    "@types/mongodb": "^3.3.16",
    "@types/node": "12.12.6",
    "concurrently": "5.1.0",
    "nodemon": "2.0.2"
  }
}
